<section class="phx-hero">
  <h1><%= gettext "Welcome to %{name}!", name: "EventStream" %></h1>
  <p>Peace of mind from prototype to production</p>

</section>

<section class="row">
  <table>
    <tr>
      <th>Data</th>
    </tr>
    <%= for event <- @events do%>
    <tr>
      <td><%= event |> inspect() %></td>
    </tr>
    <% end %>
  </table>
</section>
